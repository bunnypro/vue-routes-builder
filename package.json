{
  "name": "vue-routes-builder",
  "version": "0.8.0",
  "description": "A Vue Routes Builder",
  "main": "dist/routes.js",
  "module": "dist/routes.esm.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn test && yarn compile",
    "publish:coverage": "yarn coverage && codecov",
    "coverage": "yarn test --coverage",
    "clean": "./node_modules/.bin/rimraf ./dist ./types",
    "test": "./node_modules/.bin/jest",
    "compile": "./node_modules/.bin/rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bunnypro/vue-routes-builder"
  },
  "typings": "types/index.d.ts",
  "files": [
    "dist/*.js",
    "types/*.d.ts"
  ],
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.2",
    "jest": "^22.4.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.8.3",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/lib/util/"
    ]
  }
}
